{
  "name": "rouptimize-mobile",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "bun --env-file=./.env.development expo start",
    "android": "bun --env-file=./.env.development expo run:android",
    "ios": "bun --env-file=./.env.development expo run:ios",
    "web": "bun --env-file=./.env.development expo start --web",
    "lint": "bun expo lint",
    "docker:dev": "docker compose --env-file .env.development up -d",
    "docker:prod": "docker compose --env-file .env up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "expo:prebuild": "bun expo prebuild",
    "expo:prebuild:clean": "bun --env-file=./.env expo prebuild --clean",
    "expo:prebuild:android": "bun --env-file=./.env expo prebuild --platform android --clean",
    "expo:prebuild:ios": "bun --env-file=./.env expo prebuild --platform ios --clean",
    "eas:build:android:dev": "eas build --platform android --profile development",
    "eas:build:ios:dev": "eas build --platform ios --profile development",
    "eas:build:android:prod": "eas build --platform android --profile production",
    "eas:build:android:prod-aab": "eas build --platform android --profile production-aab",
    "eas:build:ios:prod": "eas build --platform ios --profile production",
    "build:android:apk": "cd android && NODE_ENV=production ./gradlew assembleRelease"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/drawer": "^7.7.10",
    "@react-navigation/native": "^7.1.8",
    "@rn-primitives/dialog": "^1.2.0",
    "@rn-primitives/label": "^1.2.0",
    "@rn-primitives/portal": "^1.3.0",
    "@rn-primitives/slot": "^1.2.0",
    "@rnmapbox/maps": "^10.2.10",
    "axios": "^1.13.5",
    "buffer": "^6.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "expo": "~54.0.30",
    "expo-build-properties": "~1.0.10",
    "expo-clipboard": "~8.0.8",
    "expo-constants": "~18.0.13",
    "expo-dev-client": "~6.0.20",
    "expo-font": "~14.0.11",
    "expo-image": "~3.0.11",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-location": "~19.0.8",
    "expo-router": "~6.0.21",
    "expo-secure-store": "~15.0.8",
    "expo-sensors": "~15.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-sqlite": "~16.0.10",
    "expo-status-bar": "~3.0.9",
    "expo-task-manager": "~14.0.9",
    "lucide-react-native": "^0.563.0",
    "mqtt": "^5.14.0",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.30.0",
    "react-native-reanimated": "~4.2.1",
    "react-native-safe-area-context": "~5.6.2",
    "react-native-screens": "~4.23.0",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.7.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/geojson": "^7946.0.14",
    "@types/node": "^22.10.0",
    "@types/react": "~19.1.10",
    "dotenv": "^17.2.3",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.9.2"
  },
  "private": true
}
